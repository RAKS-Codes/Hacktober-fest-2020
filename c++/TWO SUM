class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int>r;
        map<int,int>m;
        for(int i=0;i<nums.size();i++)
        { 
            int complement=target-nums[i];
            if(m.find(complement)!=m.end())
            {
                r.push_back(i);
                r.push_back(m[complement]);
                return r;
            }
            m.insert(pair<int,int>(nums[i],i));
        }
        
        return r; 
    }
};
